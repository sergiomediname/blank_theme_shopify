.navbar .nav {
  list-style: none;
  margin: 0;
  padding: 0;
  position: fixed;
  top: 0;
  left: 0;
  height: 100%;
  width: 250px;
  background-color: var(--menu-bg);
  z-index: 20;
  transform: translateX(var(--nav-position, -100%));
  transition: transform 0.5s cubic-bezier(0.54, 0, 0.1, 1);
}
.navbar .nav__link {
  display: block;
  padding: 0.75rem 1.5rem;
  text-decoration: none;
  color: var(--menu-color);
  font-weight: 700;
}

.mini-cart-content {
