.main-banner {
  min-height: 100vh;
}
.main-banner__image {
  top: 0;
  left: 0;
}
.main-banner__image img {
  width: 100%;
  height: 100%;
  object-fit: cover;
}

.offcanvas-switch::before {
  content: "";
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background-color: rgba(0, 0, 0, 0.253);
  pointer-events: var(--switch-bars, none);
  opacity: var(--switch-bars, 0);
  transition: opacity ease 0.5s;
}
.offcanvas-control {
  display: none;
}
.offcanvas-control:checked + .layout {
  --nav-position: 0;
  --switch-bars: 1;
}

.card__thumb {
  position: relative;
  aspect-ratio: 3/4;
}
.card__thumb img {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
